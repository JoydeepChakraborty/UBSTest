package com.ubs.opsit.time;

/**
 * Class to store calculated berling time
 * @author Joy
 *
 */
public class CalculatedBerlinTime {

	private String berlinHour;
	private String berlinMinute;
	private String berlinSecond;
	
	public String getBerlinHour() {
		return berlinHour;
	}
	public void setBerlinHour(String berlinHour) {
		this.berlinHour = berlinHour;
	}
	public String getBerlinMinute() {
		return berlinMinute;
	}
	public void setBerlinMinute(String berlinMinute) {
		this.berlinMinute = berlinMinute;
	}
	public String getBerlinSecond() {
		return berlinSecond;
	}
	
	
	@Override
	public String toString() {
		return "CalculatedBerlinTime [berlinHour=" + berlinHour + ", berlinMinute=" + berlinMinute + ", berlinSecond="
				+ berlinSecond + "]";
	}
	public void setBerlinSecond(String berlinSecond) {
		this.berlinSecond = berlinSecond;
	}
	
	
}
