package com.ubs.opsit.time;

public class Hours  implements Time{

	@Override
	public String calculatedString(int time) {
	
		String evaluatedHours = "";
		int SUBTRACTING_ELEMENT=1;
		if(time>5){
			SUBTRACTING_ELEMENT=5;
		}	
	else{
		SUBTRACTING_ELEMENT=1;
	}
			while(time>=5)
			{
				time=time-5;
				evaluatedHours+="R";
			}
		
	}

}
